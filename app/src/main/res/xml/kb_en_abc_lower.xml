<!--
 // USA-english HID keycode layout
-->
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0dip"
    android:verticalGap="0dip"
    android:keyHeight="6%"
    >

    <Row>
        <Key android:codes="0x1e" android:keyLabel="1" android:keyEdgeFlags="left"/>
        <Key android:codes="0x1f" android:keyLabel="2"/>
        <Key android:codes="0x20" android:keyLabel="3"/>
        <Key android:codes="0x21" android:keyLabel="4"/>
        <Key android:codes="0x22" android:keyLabel="5"/>
        <Key android:codes="0x23" android:keyLabel="6"/>
        <Key android:codes="0x24" android:keyLabel="7"/>
        <Key android:codes="0x25" android:keyLabel="8"/>
        <Key android:codes="0x26" android:keyLabel="9"/>
        <Key android:codes="0x27" android:keyLabel="0" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="0x14" android:keyLabel="q" android:keyEdgeFlags="left"/>
        <Key android:codes="0x1a" android:keyLabel="w"/>
        <Key android:codes="0x08" android:keyLabel="e"/>
        <Key android:codes="0x15" android:keyLabel="r"/>
        <Key android:codes="0x17" android:keyLabel="t"/>
        <Key android:codes="0x1c" android:keyLabel="y"/>
        <Key android:codes="0x18" android:keyLabel="u"/>
        <Key android:codes="0x0c" android:keyLabel="i"/>
        <Key android:codes="0x12" android:keyLabel="o"/>
        <Key android:codes="0x13" android:keyLabel="p" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="0x04" android:keyLabel="a" android:horizontalGap="5%p"  android:keyEdgeFlags="left"/>
        <Key android:codes="0x16" android:keyLabel="s"/>
        <Key android:codes="0x07" android:keyLabel="d"/>
        <Key android:codes="0x09" android:keyLabel="f"/>
        <Key android:codes="0x0a" android:keyLabel="g"/>
        <Key android:codes="0x0b" android:keyLabel="h"/>
        <Key android:codes="0x0d" android:keyLabel="j"/>
        <Key android:codes="0x0e" android:keyLabel="k"/>
        <Key android:codes="0x0f" android:keyLabel="l" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="-1" android:keyIcon="@drawable/key_qwerty_shift" android:iconPreview="@drawable/key_qwerty_shift_b" android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="0x1d" android:keyLabel="z"/>
        <Key android:codes="0x1b" android:keyLabel="x"/>
        <Key android:codes="0x06" android:keyLabel="c"/>
        <Key android:codes="0x19" android:keyLabel="v"/>
        <Key android:codes="0x05" android:keyLabel="b"/>
        <Key android:codes="0x11" android:keyLabel="n"/>
        <Key android:codes="0x10" android:keyLabel="m"/>
        <Key android:codes="-6" android:keyIcon="@drawable/key_qwerty_alt" android:isModifier="true" android:isSticky="true" />
        <Key android:codes="-20" android:keyIcon="@drawable/key_qwerty_ctrl" android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="right"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-30" android:keyLabel="SYM" android:keyWidth="15%p" android:keyEdgeFlags="left"/>
        <Key android:codes="-50" android:keyLabel="CLEAR" android:keyWidth="15%p" />
        <Key android:codes="0x2c" android:keyIcon="@drawable/key_qwerty_space" android:iconPreview="@drawable/key_qwerty_space_b" android:keyWidth="55%p"/>
        <Key android:codes="0x28" android:keyIcon="@drawable/key_qwerty_enter" android:iconPreview="@drawable/key_qwerty_enter_b" android:keyWidth="15%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>

